<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:GroupBox style="padding-left: 0; height: initial; width: initial;">
        <ui:TextField picking-mode="Ignore" name="NewSceneTextField" style="margin-bottom: 3px; margin-top: 8px;" />
        <ui:GroupBox style="flex-direction: row; margin-top: 8px; align-items: center; justify-content: center; padding-left: 0;">
            <ui:Button text="Confirm" display-tooltip-when-elided="true" name="ConfirmBtn" style="width: 50%; margin-right: 4px; margin-left: 4px;" />
            <ui:Button text="Cancel" display-tooltip-when-elided="true" name="CancelBtn" style="width: 50%; margin-left: 4px; margin-right: 4px;" />
        </ui:GroupBox>
    </ui:GroupBox>
